{
  "plugins": ["plugins/markdown"],
  "markdown": {
    "idInHeadings": true
  },
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+jsdoc/dbindjs_.+_stub.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "default": {
      "includeDate": false,
      "outputSourceFiles": false,
      "useLongnameInNav": true
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "templates/default",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
